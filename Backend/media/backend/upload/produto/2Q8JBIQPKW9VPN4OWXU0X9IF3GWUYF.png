<script>
  function abreModal() {

    var alturaTela = $(document).height();
    var larguraTela = $(window).width();

    //colocando o fundo preto
    $('#mascara').css({ 'width': larguraTela, 'height': alturaTela });
    $('#mascara').fadeIn(1000);
    $('#mascara').fadeTo("slow", 0.8);

    var ofsetJanela = parseInt(window.getComputedStyle(janela1).width);
    //console.log("tamanho offset" + ofsetJanela);
    var tela = parseInt($(window).width());
    //console.log("tamanho tela" + tela);
    var left = (tela - ofsetJanela) / 2;
    //console.log("tamanho left" + left);
    var top = document.querySelector("#fbits-componente-parcelamento-0 > div").offsetTop

    $("#janela1").show();
    $("#janela1").css({ 'top': top, 'left': left });

    $('.fechar').click(function (ev) {
      ev.preventDefault();
      $("#mascara").hide();
      $(".window").hide();
    });
  }
</script>

