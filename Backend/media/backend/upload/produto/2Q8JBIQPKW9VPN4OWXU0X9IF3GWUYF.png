// seta cookie qdo retirada loja
function setCookie(cname, cvalue, exdays) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
    var expires = "expires="+d.toUTCString();
    document.cookie = cname + "=" + cvalue + ";" + expires + ";domain=.eletrorastro.com.br;path=/";
    
}
function criaBTSpot(){
    $(".spot").each(function(){
        if (!$(this).find(".retire-loja").html() ) {
            $(this).find(".bt.adicionar").clone().addClass("retire-loja").appendTo( $(this).find(".botoesSpot") );
            $(this).find(".bt.adicionar").attr("data-abrirmodal","False");
        }
    });
    bindBTSpot();
}

function bindBTSpot(){
    for (ii=0; ii <= somenteLoja.length-1; ii++) {
        $(".botoesSpot").each(function(){

            var strr = $(this).find("a").attr("href");
            //console.log(ii);
            if (strr) {
                if (~strr.indexOf(somenteLoja[ii])) {
                   $(this).find(".bt.adicionar.retire-loja").prev().prev().css("opacity","0.3").css("pointer-events","none");
                }
            }
        });
    }
}

