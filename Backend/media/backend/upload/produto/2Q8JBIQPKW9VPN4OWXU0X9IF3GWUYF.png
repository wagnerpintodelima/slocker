$(document).ready(function() {
    console.log('function');
        console.log('slideImg');
        if($(window).width() > 1024){
            var div1 = $("body > div.head > div.topBar > div > div > div.promo-content > div.divImg1Desk");
            var div2 = $("body > div.head > div.topBar > div > div > div.promo-content > div.divImg3Desk");
            console.log('tela normal');
        }else{
            var div1 = $("body > div.head > div.topBar > div > div > div.promo-content-mobile > div.img1Mob");
            var div2 = $("body > div.head > div.topBar > div > div > div.promo-content-mobile > div.img2Mob");
            console.log('tela mobile');

        }
        var escondida=div1;
        var exibida=div2;		
        

    exibida.css("visibility","visible");
    escondida.css("visibility","hidden");

    setInterval(function() {
        exibida.css("visibility","visible");       
        
        //console.log('exibe a escondida');
        escondida.css("visibility","hidden");  
        //console.log('troca a escondida com a exibida');
        temp = escondida;
        escondida = exibida;
        exibida = temp;
        exibida.css("visibility","visible");
        escondida.css("visibility","hidden");
    }, 3000)  
})
</script>




