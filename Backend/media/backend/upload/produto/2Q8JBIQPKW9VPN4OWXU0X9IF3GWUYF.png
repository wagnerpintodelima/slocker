<script>
  $(".btnCalculaFreteProduto").click(function () {
    $(".rotate-time-container").css("display", "none");
    $(".rotate-time-container").removeClass("rotate-time-content");
    $(".rotate-time-container").toggle();
    $(".rotate-time-container").addClass("rotate-time-content")
  });
  /************************ mutation observer ************************/
  /**/
  /************************ spinner para frete ************************/
  // seleciona o nó alvo
  var target_spinner = document.querySelector('#valorFrete');
  // cria uma nova instância de observador
  var observer_spinner = new MutationObserver(function (mutations) {
